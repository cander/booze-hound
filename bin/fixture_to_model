#!/usr/bin/env ruby

require "yaml"

# could probably get the model from the file name
model_name = ARGV[0]
yaml_file = ARGV[1]

puts "Creating #{model_name} models from #{yaml_file}"
models = YAML.load_file(yaml_file)
models.each do |var_name, hash|
  puts "#{var_name} = #{model_name}.create(\n  #{hash}\n)"
end